{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "build": {
      "functions": {
        "pip": "pip install -r /vercel/path0/requirements.txt"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }
  
